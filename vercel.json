{
  "version": 2,
  "builds": [
    {
      "src": "api/todo.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    },
    {
      "src": "templates/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/todo.py"
    }
  ]
}
